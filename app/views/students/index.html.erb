<h1>Listing students</h1>
<table>
  <tr>
    <th>Id</th>
    <th>School</th>
    <th>classroom</th>
    <th>name</th>
    <th>address</th>
    <th>phone_no</th>
    <th>gender</th>
    <th colspan="3"></th>
  </tr>
  <% @students.each do |student| %>
    <tr>
      <td><%= student.id %></td>
      <td><%= student.school.name %></td>
      <td><%= student.classroom.class_no %></td>
      <td><%= student.name %></td>
      <td><%= student.address %></td>
      <td><%= student.phone_no %></td>
      <td><%= student.gender %></td>
      <td><%= link_to 'Show', student_path(student) %></td>
    </tr>
  <% end %>
</table>